<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.customers.home.type_waste.TypeWasteFragment">

    <include
        android:id="@+id/viewBar"
        layout="@layout/custom_app_bar_button_and_title"
        android:layout_width="match_parent"
        android:layout_height="47dp" />


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerViewTypeWaste"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="20dp"
        android:layout_marginHorizontal="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:listitem="@layout/item_content_type_waste"
        tools:itemCount="5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/viewBar" />


</androidx.constraintlayout.widget.ConstraintLayout>